<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/cuttingboardo"
    tools:context=".activities.loginactivity.Register"
    tools:layout_editor_absoluteX="3dp"
    tools:layout_editor_absoluteY="-63dp">


    <TextView
        android:id="@+id/register_titleID"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/rammetto_one"
        android:text="@string/get_fat"
        android:textColor="@color/dark_light_header"
        android:textSize="@dimen/text_logo"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.123" />

    <TextView
        android:id="@+id/register_undertitleID"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/create_and_eat"
        android:textColor="@color/light_undertitle_text"
        android:textSize="@dimen/text_medium_16"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.246" />

    <EditText
        android:id="@+id/register_pt_nameID"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/padding_mediumbig_16"
        android:layout_marginEnd="@dimen/padding_mediumbig_16"
        android:autofillHints=""
        android:background="@color/colorIcons"
        android:ems="10"
        android:hint="@string/hint_name"
        android:inputType="textPersonName"
        android:padding="@dimen/padding_small_10"
        android:textColor="#FFFFFF"
        android:textColorHint="#FFFFFF"
        android:textSize="@dimen/text_size_small_14"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/register_undertitleID"
        app:layout_constraintVertical_bias="0.141" />

    <Button
        android:id="@+id/Register_Btn"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/padding_mediumbig_16"
        android:layout_marginEnd="@dimen/padding_mediumbig_16"
        android:layout_marginBottom="232dp"
        android:autofillHints=""
        android:background="@color/light_background_yellow"
        android:padding="@dimen/padding_small_10"
        android:text="@string/Register_btn"
        android:textColor="@color/dark_background"
        android:textColorHint="#FFFFFF"
        android:textSize="@dimen/text_size_small_14"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/login_pt_passwordID" />

    <TextView
        android:id="@+id/createText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Already Registered ? Login Here"
        android:textColor="#FFEB3B"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Register_Btn" />

    <EditText
        android:id="@+id/login_pt_mailID"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/padding_mediumbig_16"
        android:layout_marginEnd="@dimen/padding_mediumbig_16"
        android:autofillHints=""
        android:background="@color/colorIcons"
        android:ems="10"
        android:hint="@string/hint_email"
        android:inputType="textPersonName"
        android:padding="@dimen/padding_small_10"
        android:textColor="#FFFFFF"

        android:textColorHint="#FFFFFF"
        android:textSize="@dimen/text_size_small_14"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.557"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/register_pt_nameID"
        app:layout_constraintVertical_bias="0.039" />

    <EditText
        android:id="@+id/login_pt_passwordID"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/padding_mediumbig_16"
        android:layout_marginEnd="@dimen/padding_mediumbig_16"
        android:background="@color/colorIcons"
        android:ems="10"
        android:hint="@string/hint_password"
        android:inputType="textPersonName"
        android:padding="@dimen/padding_small_10"
        android:textColor="#FFFFFF"
        android:textColorHint="#FFFFFF"
        android:textSize="@dimen/text_size_small_14"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.567"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/login_pt_mailID"
        app:layout_constraintVertical_bias="0.046" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="0dp"
        android:layout_height="30dp"
        android:layout_marginStart="60dp"
        android:layout_marginEnd="60dp"
        android:text="@string/by_agreeing_part_1"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textColorHint="#FFFFFF"
        android:textSize="@dimen/text_smallest_12"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Register_Btn"
        app:layout_constraintVertical_bias="0.327" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="0dp"
        android:layout_height="32dp"
        android:layout_marginStart="@dimen/paddint_mega_ultra_60"
        android:layout_marginEnd="@dimen/paddint_mega_ultra_60"
        android:text="@string/by_agreeing_part_2"
        android:textAlignment="center"
        android:textColor="@color/colorWhiteDark"
        android:textColorHint="@color/colorWhiteDark"
        android:textSize="@dimen/text_smallest_12"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        app:layout_constraintVertical_bias="0.0" />


</androidx.constraintlayout.widget.ConstraintLayout>